{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "no-irregular-whitespace": 2,
    "no-console": 0,
    "block-spacing": 2,
    "spacing-in-parents": 2,
    "space-in-brackets":2,

    "space-before-function-paren": 2
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ]
}

